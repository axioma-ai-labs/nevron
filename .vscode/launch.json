{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Nevron",
            "type": "python",
            "request": "launch",
            "module": "src.main",
            "console": "integratedTerminal",
            "justMyCode": true,
            "python": "${command:python.interpreterPath}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Python: Debug",
            "type": "debugpy",
            "request": "launch",
            "module": "src.debug",
            "console": "integratedTerminal"
        }
    ]
} 